{
	"$schema": "https://json.schemastore.org/nest-cli",
	"collection": "@nestjs/schematics",
	"sourceRoot": "src",
	"compilerOptions": {
		"deleteOutDir": true,
		"assets": [
			{
				"include": "**/*.md"
			},
			{
				"include": "**/*.html"
			},
			{
				"include": "**/*.json"
			},
			{
				"include": "**/*.xlsx"
			}
		],
		"plugins": ["@nestjs/swagger"]
	}
}
