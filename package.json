{
  "name": "mybeans-backend-public",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nest start --watch",
    "local": "nest start --watch --env-file .env.local",
    "local:docker-infra": "docker compose -f ./docker/local/docker-compose-infra.yml up",
    "local:docker-infra-down": "docker compose -f ./docker/local/docker-compose-infra.yml down",
    "local:docker-server": "docker compose -f ./docker/local/docker-compose-server.yml up --build --force-recreate",
    "local:docker-server-down": "docker compose -f ./docker/local/docker-compose-server.yml down",
    "dev:docker-infra": "docker compose -f ./docker/dev/docker-compose-infra.yml up -d",
    "dev:docker-infra-down": "docker compose -f ./docker/dev/docker-compose-infra.yml down",
    "dev:docker-server": "docker compose -f ./docker/dev/docker-compose-server.yml up --build --force-recreate",
    "dev:docker-server-down": "docker compose -f ./docker/dev/docker-compose-server.yml down",
    "prisma:pull-local": "cp .env.local .env && npx prisma db pull && rm .env",
    "prisma:push-local": "cp .env.local .env && npx prisma db push && rm .env",
    "prisma:generate": "cd ./prisma && prisma-case-format && npx prisma generate",
    "tree": "tree -a -I 'node_modules|.git|mysql-data|*.log'",
    "script": "ts-node script/runner.ts",
    "cron": "ts-node cron/runner.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lsh940131/mybeans-backend-public.git"
  },
